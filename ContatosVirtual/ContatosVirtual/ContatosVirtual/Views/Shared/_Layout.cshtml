@{
    string nome = (string)Session["nome"];
}
<!DOCTYPE html>

<html lang="pt-br">
<head>
    <meta charset="UTF-8")>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" href="~/Content/img/icone.png" type="image/x-icon" />
    <link rel="shortcut icon" href="~/Content/img/icone.png" type="image/x-icon" />
    <link href="~/Content/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">
    <link href="~/Content/css/sb-admin.css" rel="stylesheet">
    <link href="~/Content/css/site.css" rel="stylesheet">
    <link href="~/Content/css/noty.css" rel="stylesheet">
    <link href="~/Content/css/tooltipster.bundle.min.css" rel="stylesheet">
    <link href="~/Content/css-tooltip/themes/tooltipster-sideTip-borderless.min.css" rel="stylesheet">
    <link href="~/Content/css-tooltip/themes/tooltipster-sideTip-light.min.css" rel="stylesheet">
    <link href="~/Content/css-tooltip/themes/tooltipster-sideTip-noir.min.css" rel="stylesheet">
    <link href="~/Content/css-tooltip/themes/tooltipster-sideTip-punk.min.css" rel="stylesheet">
    <link href="~/Content/css-tooltip/themes/tooltipster-sideTip-shadow.min.css" rel="stylesheet">
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
</head>
<body class="fixed-nav sticky-footer bg-dark" id="page-top">
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top" id="mainNav">
        <a class="navbar-brand" href="Home">Contatos Virtual</a>
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
            <ul class="navbar-nav navbar-sidenav" id="exampleAccordion">
                <li class="nav-item" data-toggle="tooltip" data-placement="right" title="Inicial">
                    <a class="nav-link" href="Home">
                        <i class="fa fa-fw fa-home"></i>
                        <span class="nav-link-text">Inicial</span>
                    </a>
                </li>
                <li class="nav-item" data-toggle="tooltip" data-placement="right" title="Cadastros">
                    <a class="nav-link nav-link-collapse collapsed" data-toggle="collapse" href="#collapseComponents" data-parent="#exampleAccordion">
                        <i class="fa fa-fw fa-pencil"></i>
                        <span class="nav-link-text">Cadastros</span>
                    </a>
                    <ul class="sidenav-second-level collapse" id="collapseComponents">
                        <li>
                            <a href="Empresas">Empresas</a>
                        </li>
                        <li>
                            <a href="Fornecedores">Fornecedores</a>
                        </li>
                    </ul>
                </li>
                <li class="nav-item" data-toggle="tooltip" data-placement="right" title="Usuários">
                    <a class="nav-link" href="Usuarios">
                        <i class="fa fa-fw fa-users"></i>
                        <span class="nav-link-text">Usuários</span>
                    </a>
                </li>
            </ul>
            <ul class="navbar-nav sidenav-toggler ">
                <li class="nav-item">
                    <a class="nav-link text-center " id="sidenavToggler">
                        <i class="fa fa-fw fa-angle-left"></i>
                    </a>
                </li>
            </ul>
            <ul class="navbar-nav ml-auto align-content-center">
                <li class="nav-item">
                    <a class="nav-link text-white">Bem vindo: @nome</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="botao-sair">
                        <i class="fa fa-fw fa-sign-out"></i>SAIR
                    </a>
                </li>
            </ul>
        </div>
    </nav>
    @RenderBody()
    <footer class="sticky-footer">
        <div class="container">
            <div class="text-center">
                <small>Copyright © Contatos Virtual - Bilk @DateTime.Now.Year</small>
            </div>
        </div>
    </footer>
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fa fa-angle-up"></i>
    </a>

    <div class="modal fade" id="modalSair" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">

    </div>

    <script src="~/Content/vendor/jquery/jquery.min.js" charset="ISO-8859-1"></script>
    <script src="~/Content/vendor/popper/popper.min.js" charset="ISO-8859-1"></script>
    <script src="~/Content/vendor/bootstrap/js/bootstrap.min.js" charset="ISO-8859-1"></script>
    <script src="~/Content/vendor/jquery-easing/jquery.easing.min.js" charset="ISO-8859-1"></script>
    <script src="~/Content/lib/noty.js" type="text/javascript" charset="ISO-8859-1"></script>
    <script src="~/Content/js/modalSair.js" charset="ISO-8859-1"></script>
    <script src="~/Content/js/empresas.js" charset="ISO-8859-1"></script>
    <script src="~/Content/js/fornecedores.js" charset="ISO-8859-1"></script>
    <script src="~/Content/js/mascaras.js" charset="ISO-8859-1"></script>
    <script src="~/Content/js/cep.js" charset="ISO-8859-1"></script>
    <script src="~/Content/js/chamadaVerificacoes.js" charset="ISO-8859-1"></script>
    <script src="~/Content/js/telefones.js" charset="ISO-8859-1"></script>
    <script src="~/Content/vendor/datatables/jquery.dataTables.js"></script>
    <script src="~/Content/vendor/datatables/dataTables.bootstrap4.js"></script>
    <script src="~/Content/js/sb-admin.min.js" charset="ISO-8859-1"></script>
    <script src="~/Content/js/sb-admin-datatables.min.js"></script>
    <script src="~/Content/js/filtro-tabela-empresas.js" charset="ISO-8859-1"></script>
    <script src="~/Content/js/filtro-tabelas-fornecedores.js" charset="ISO-8859-1"></script>
    <script src="~/Content/js/filtro-tabela-usuarios.js" charset="ISO-8859-1"></script>
    <script src="~/Content/js/usuarios.js" charset="ISO-8859-1"></script>
</body>
</html>